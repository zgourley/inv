<h1>My Inventories</h1>

<ul>
  <% @list.each do |list| %>
  <li>
      <%= list.campaign %> - <%= list.character %>
      <%= link_to "Delete this inventory", delete_list_path(list.id), method: :delete, data: { confirm: "Are you sure you want to delete this inventory?"} %>
  </li>
  <% end %>
</ul>