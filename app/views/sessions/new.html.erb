<h1>Log In</h1>
<div class="signupbox">
<%= form_for :login do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <div type="submit" class="btn btn-default">
    <%= f.submit 'Log In' %>
  </div>
<% end %>
</div>